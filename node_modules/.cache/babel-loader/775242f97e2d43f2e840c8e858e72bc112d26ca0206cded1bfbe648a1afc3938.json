{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nexport default {\n  __name: 'IndexPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const lights = ref([{\n      id: 1,\n      isOn: 1,\n      onTime: 0,\n      offTime: 0\n    }, {\n      id: 2,\n      isOn: 1,\n      onTime: 0,\n      offTime: 0\n    }, {\n      id: 3,\n      isOn: 1,\n      onTime: 0,\n      offTime: 0\n    }, {\n      id: 4,\n      isOn: 1,\n      onTime: 0,\n      offTime: 0\n    }, {\n      id: 5,\n      isOn: 1,\n      onTime: 0,\n      offTime: 0\n    }]);\n    const seconds = ref([{\n      label: 10,\n      value: 1\n    }, {\n      label: 20,\n      value: 2\n    }, {\n      label: 30,\n      value: 3\n    }]);\n    const changeStatus = id => {\n      const light = lights.value[id - 1];\n      console.log(light);\n      const url = '/led';\n      const data = {\n        id: light.id,\n        button: light.isOn\n      };\n      const config = {\n        params: data\n      };\n      axios.post(url, data, config).then(response => {\n        console.log(response.data);\n      });\n    };\n    const changeOnTime = id => {\n      const light = lights.value[id - 1];\n      console.log(light);\n      // 处理选中值的变化\n    };\n\n    const changeOffTime = id => {\n      const light = lights.value[id - 1];\n      console.log(light);\n      // 处理选中值的变化\n    };\n\n    const __returned__ = {\n      lights,\n      seconds,\n      changeStatus,\n      changeOnTime,\n      changeOffTime,\n      ref,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","axios","lights","id","isOn","onTime","offTime","seconds","label","value","changeStatus","light","console","log","url","data","button","config","params","post","then","response","changeOnTime","changeOffTime"],"sources":["D:/javafile/LightControl/src/views/IndexPage.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div v-for=\"light in lights\" :key=\"light.id\" class=\"light\">\n      <div class=\"light-info\">灯{{ light.id }}</div>\n      <el-switch\n          v-model=\"light.isOn\"\n          :active-value=\"0\"\n          :inactive-value=\"1\"\n          active-color=\"#13ce66\"\n          inactive-color=\"#ff4949\"\n          @change=\"changeStatus(light.id)\">\n      </el-switch>\n      <el-select\n          v-model=\"light.onTime\"\n          placeholder=\"几秒钟后开\"\n          @change=\"changeOnTime(light.id)\">\n        <el-option\n            v-for=\"item in seconds\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\">\n        </el-option>\n      </el-select>\n      <el-select\n          v-model=\"light.offTime\"\n          placeholder=\"几秒钟后关\"\n          @change=\"changeOffTime(light.id)\">\n        <el-option\n            v-for=\"item in seconds\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\">\n        </el-option>\n      </el-select>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from 'vue';\nimport axios from \"axios\";\n\nconst lights = ref([\n  { id: 1, isOn: 1, onTime:0, offTime:0 },\n  { id: 2, isOn: 1, onTime:0, offTime:0 },\n  { id: 3, isOn: 1, onTime:0, offTime:0 },\n  { id: 4, isOn: 1, onTime:0, offTime:0 },\n  { id: 5, isOn: 1, onTime:0, offTime:0 },\n]);\nconst seconds = ref([\n  { label: 10, value: 1 },\n  { label: 20, value: 2 },\n  { label: 30, value: 3 }\n])\nconst changeStatus = (id) => {\n  const light = lights.value[id - 1];\n  console.log(light)\n  const url = '/led'\n  const data = {\n    id: light.id,\n    button: light.isOn\n  };\n  const config = {\n    params: data\n  }\n  axios.post(url, data, config).then(response => {\n    console.log(response.data)\n  })\n}\nconst changeOnTime = (id) => {\n  const light = lights.value[id - 1];\n  console.log(light)\n  // 处理选中值的变化\n}\nconst changeOffTime = (id) => {\n  const light = lights.value[id - 1];\n  console.log(light)\n  // 处理选中值的变化\n}\n</script>\n\n\n<style scoped>\n.container {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.light {\n  display: flex;\n  align-items: center;\n  margin: 10px;\n}\n\n.light-info {\n  margin-right: 10px;\n}\n\n.light-status {\n  margin-left: 10px;\n}\n\n.timer-label {\n  margin-left: 10px;\n}\n\n.timer-status {\n  margin-left: 10px;\n  font-size: 12px;\n  color: #999;\n}\n</style>\n"],"mappings":"AAuCA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,KAAK,MAAM,OAAO;;;;;;;IAEzB,MAAMC,MAAM,GAAGF,GAAG,CAAC,CACjB;MAAEG,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,CAAC;MAAEC,MAAM,EAAC,CAAC;MAAEC,OAAO,EAAC;IAAE,CAAC,EACvC;MAAEH,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,CAAC;MAAEC,MAAM,EAAC,CAAC;MAAEC,OAAO,EAAC;IAAE,CAAC,EACvC;MAAEH,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,CAAC;MAAEC,MAAM,EAAC,CAAC;MAAEC,OAAO,EAAC;IAAE,CAAC,EACvC;MAAEH,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,CAAC;MAAEC,MAAM,EAAC,CAAC;MAAEC,OAAO,EAAC;IAAE,CAAC,EACvC;MAAEH,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,CAAC;MAAEC,MAAM,EAAC,CAAC;MAAEC,OAAO,EAAC;IAAE,CAAC,CACxC,CAAC;IACF,MAAMC,OAAO,GAAGP,GAAG,CAAC,CAClB;MAAEQ,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAE,CAAC,EACvB;MAAED,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAE,CAAC,EACvB;MAAED,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAE,CAAC,CACxB,CAAC;IACF,MAAMC,YAAY,GAAIP,EAAE,IAAK;MAC3B,MAAMQ,KAAK,GAAGT,MAAM,CAACO,KAAK,CAACN,EAAE,GAAG,CAAC,CAAC;MAClCS,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MAClB,MAAMG,GAAG,GAAG,MAAM;MAClB,MAAMC,IAAI,GAAG;QACXZ,EAAE,EAAEQ,KAAK,CAACR,EAAE;QACZa,MAAM,EAAEL,KAAK,CAACP;MAChB,CAAC;MACD,MAAMa,MAAM,GAAG;QACbC,MAAM,EAAEH;MACV,CAAC;MACDd,KAAK,CAACkB,IAAI,CAACL,GAAG,EAAEC,IAAI,EAAEE,MAAM,CAAC,CAACG,IAAI,CAACC,QAAQ,IAAI;QAC7CT,OAAO,CAACC,GAAG,CAACQ,QAAQ,CAACN,IAAI,CAAC;MAC5B,CAAC,CAAC;IACJ,CAAC;IACD,MAAMO,YAAY,GAAInB,EAAE,IAAK;MAC3B,MAAMQ,KAAK,GAAGT,MAAM,CAACO,KAAK,CAACN,EAAE,GAAG,CAAC,CAAC;MAClCS,OAAO,CAACC,GAAG,CAACF,KAAK,C