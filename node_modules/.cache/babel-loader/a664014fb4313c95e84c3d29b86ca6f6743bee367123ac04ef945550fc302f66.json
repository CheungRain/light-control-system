rmData();\r\n      //向FormData中添加数据\r\n      formData.append(\"image\", files[0])\r\n      axios.post(\"http://localhost:8081/person/image\", formData).then(req => {\r\n        console.log(req.data);\r\n      })\r\n    }\r\n    function putPerson() {\r\n      axios.put(\"http://localhost:8081/person/45\", {\r\n        id: '13313',\r\n        name: '慕随心'\r\n      }, {\r\n        params: {\r\n          tiZhong: 55,\r\n          height: 175,\r\n        }\r\n      }).then(req => {\r\n        console.log(req.data);\r\n      })\r\n    }\r\n    function deletePerson() {\r\n      axios.delete(\"http://localhost:8081/person/12\").then(req => {\r\n        console.log(req.data);\r\n      })\r\n    }\r\n    return {\r\n      getPerson,\r\n      postPerson,\r\n      putPerson,\r\n      deletePerson,\r\n      postFile,\r\n      file\r\n    }\r\n  }\r\n});\r\n</script>\r\n\r\n"],"mappings":"AAyBA,SAASA,GAAG,QAAO,KAAI;AACvB,OAAOC,KAAI,MAAO,qBAAoB;AACtC,eAAgB;EACdC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIP,GAAG,CAAC;IACjB,SAASQ,SAASA,CAAA,EAAG;MACnBP,KAAK,CAACQ,GAAG,CAAC,iCAAiC,EAAE;QAC3CC,MAAM,EAAE;UACNC,MAAM,EAAE,EAAE;UACVC,OAAO,EAAE;QACX;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;MACvB,CAAC;IACH;IACA,SAASC,UAAUA,CAAA,EAAG;MACpBjB,KAAK,CAACkB,IAAI,CAAC,6BAA6B;MACpC;MACA;QACEC,EAAE,EAAE,OAAO;QACXlB,IAAI,EAAE;MACR,CAAC,EAAE;QACDQ,MAAM,EAAE;UACNW,MAAM,EAAE,WAAW;UACnBC,QAAQ,EAAE;QACZ;MACF,CAAC,CAAC,CAACT,IAAI,CAACC,GAAE,IAAK;QACjBC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;MACvB,CAAC;IACH;IACA;;;IAGA,SAASM,QAAQA,CAAA,EAAG;MAClB;MACA,IAAIC,KAAI,GAAIjB,IAAI,CAACkB,KAAK,CAACD,KAAK;MAC5B;MACA,IAAIE,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC7B;MACAD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAEJ,KAAK,CAAC,CAAC,CAAC;MACjCvB,KAAK,CAACkB,IAAI,CAAC,oCAAoC,EAAEO,QAAQ,CAAC,CAACb,IAAI,CAACC,GAAE,IAAK;QACrEC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;MACvB,CAAC;IACH;IACA,SAASY,SAASA,CAAA,EAAG;MACnB5B,KAAK,CAAC6B