{"ast":null,"code":"import { ref } from 'vue';\nimport { ElSwitch, ElButton, ElDialog, ElForm, ElFormItem, ElSelect, ElOption, ElMessage } from 'element-plus';\nexport default {\n  components: {\n    ElSwitch,\n    ElButton,\n    ElDialog,\n    ElForm,\n    ElFormItem,\n    ElSelect,\n    ElOption\n  },\n  setup() {\n    const lights = ref([{\n      id: 1,\n      name: '灯1',\n      isOn: 0,\n      isTimerOnOff: null,\n      timerTime: null,\n      timerStatus: false\n    }, {\n      id: 2,\n      name: '灯2',\n      isOn: 0,\n      isTimerOnOff: null,\n      timerTime: null,\n      timerStatus: false\n    }, {\n      id: 3,\n      name: '灯3',\n      isOn: 0,\n      isTimerOnOff: null,\n      timerTime: null,\n      timerStatus: false\n    }, {\n      id: 4,\n      name: '灯4',\n      isOn: 0,\n      isTimerOnOff: null,\n      timerTime: null,\n      timerStatus: false\n    }, {\n      id: 5,\n      name: '灯5',\n      isOn: 0,\n      isTimerOnOff: null,\n      timerTime: null,\n      timerStatus: false\n    }]);\n    const timerChoices = ref([{\n      label: '10秒',\n      value: 10\n    }, {\n      label: '30秒',\n      value: 30\n    }, {\n      label: '60秒',\n      value: 60\n    }]);\n    const onOff = ref([{\n      label: '定时开',\n      value: 1\n    }, {\n      label: '定时关',\n      value: 0\n    }]);\n    const showTimerDialog = ref(false);\n    const cancelTime = light => {\n      light.isTimerOnOff = null;\n      light.timerTime = null;\n      light.timerStatus = false;\n    };\n    const confirmTime = light => {\n      if (light.isTimerOnOff === null || light.timerTime === null) {\n        ElMessage.error('请选择定时开关和定时时间');\n      } else {\n        light.timerStatus = true;\n        light.showTimerDialog = false;\n        setTimeout(() => {\n          light.timerStatus = false;\n          light.isOn = light.isTimerOnOff;\n        }, light.timerTime * 1000);\n      }\n    };\n    ElMessage.success('欢迎登录');\n    return {\n      lights,\n      timerChoices,\n      onOff,\n      showTimerDialog,\n      cancelTime,\n      confirmTime\n    };\n  }\n};","map":{"version":3,"names":["ref","ElSwitch","ElButton","ElDialog","ElForm","ElFormItem","ElSelect","ElOption","ElMessage","components","setup","lights","id","name","isOn","isTimerOnOff","timerTime","timerStatus","timerChoices","label","value","onOff","showTimerDialog","cancelTime","light","confirmTime","error","setTimeout","success"],"sources":["D:\\javafile\\LightControl\\src\\views\\testPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div v-for=\"light in lights\" :key=\"light.id\" class=\"light\">\r\n      <div class=\"light-wrapper\">\r\n        <div class=\"light-status\">\r\n          <div>{{ light.name }}</div>\r\n          <div>{{ light.isOn ? '开' : '关' }}</div>\r\n        </div>\r\n        <div class=\"light-switch\">\r\n          <el-switch v-model=\"light.isOn\" active-color=\"#13ce66\" inactive-color=\"#ff4949\"></el-switch>\r\n        </div>\r\n        <div class=\"light-timer\">\r\n          <div class=\"light-timer-choose\">\r\n            <el-button @click=\"showTimerDialog = true\">设置定时</el-button>\r\n          </div>\r\n          <div class=\"light-timer-countdown\">\r\n            <el-text v-if=\"light.timerStatus===true && light.isTimerOnOff===1\">\r\n              {{light.name}}还有{{light.timerTime}}秒开启\r\n            </el-text>\r\n            <el-text v-else-if=\"light.timerStatus===true && light.isTimerOnOff===0\">\r\n              {{light.name}}还有{{light.timerTime}}秒关闭\r\n            </el-text>\r\n            <el-text v-else>\r\n              <br>\r\n            </el-text>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <el-dialog v-model=\"showTimerDialog\" title=\"设置定时\" @close=\"showTimerDialog = false\">\r\n        <el-form>\r\n          <el-form-item label=\"定时开关\">\r\n            <el-select v-model=\"light.isTimerOnOff\" placeholder=\"定时开关\">\r\n              <el-option v-for=\"item in onOff\" :key=\"item\" :label=\"item.label\" :value=\"item.value\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"定时时间\">\r\n            <el-select v-model=\"light.timerTime\" placeholder=\"定时时间\">\r\n              <el-option v-for=\"item in timerChoices\" :key=\"item\" :label=\"item.label\" :value=\"item.value\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-button @click=\"confirmTime(light)\">确认</el-button>\r\n          <el-button @click=\"cancelTime(light)\">取消</el-button>\r\n        </el-form>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\nimport { ElSwitch, ElButton, ElDialog, ElForm, ElFormItem, ElSelect, ElOption, ElMessage } from 'element-plus';\r\n\r\n\r\nexport default {\r\n  components: {\r\n    ElSwitch,\r\n    ElButton,\r\n    ElDialog,\r\n    ElForm,\r\n    ElFormItem,\r\n    ElSelect,\r\n    ElOption,\r\n  },\r\n  setup() {\r\n    const lights = ref([\r\n      { id: 1, name: '灯1', isOn: 0, isTimerOnOff: null, timerTime: null, timerStatus: false},\r\n      { id: 2, name: '灯2', isOn: 0, isTimerOnOff: null, timerTime: null, timerStatus: false},\r\n      { id: 3, name: '灯3', isOn: 0, isTimerOnOff: null, timerTime: null, timerStatus: false},\r\n      { id: 4, name: '灯4', isOn: 0, isTimerOnOff: null, timerTime: null, timerStatus: false},\r\n      { id: 5, name: '灯5', isOn: 0, isTimerOnOff: null, timerTime: null, timerStatus: false},\r\n    ]);\r\n\r\n    const timerChoices = ref([\r\n      { label: '10秒', value: 10},\r\n      { label: '30秒', value: 30},\r\n      { label: '60秒', value: 60},\r\n    ]);\r\n\r\n    const onOff = ref([\r\n      { label: '定时开', value: 1},\r\n      { label: '定时关', value: 0},\r\n    ]);\r\n\r\n    const showTimerDialog = ref(false);\r\n\r\n    const cancelTime = (light) =>{\r\n      light.isTimerOnOff = null;\r\n      light.timerTime = null;\r\n      light.timerStatus = false;\r\n    };\r\n\r\n    const confirmTime = (light) => {\r\n      if (light.isTimerOnOff === null || light.timerTime === null) {\r\n        ElMessage.error('请选择定时开关和定时时间');\r\n      } else {\r\n        light.timerStatus = true;\r\n        light.showTimerDialog = false;\r\n        setTimeout(() => {\r\n          light.timerStatus = false;\r\n          light.isOn = light.isTimerOnOff;\r\n        }, light.timerTime * 1000);\r\n      }\r\n    };\r\n    ElMessage.success('欢迎登录');\r\n\r\n    return {\r\n      lights,\r\n      timerChoices,\r\n      onOff,\r\n      showTimerDialog,\r\n      cancelTime,\r\n      confirmTime,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n  background-color: #fff;\r\n}\r\n\r\n.container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  height: 100vh;\r\n  padding: 20px;\r\n  box-sizing: border-box;\r\n  background-color: white;\r\n}\r\n\r\n.light {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.light-wrapper {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  border-radius: 10px;\r\n  background-color: #fff;\r\n}\r\n\r\n.light-status {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.light-switch {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.light-timer {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n</style>\r\n"],"mappings":"AAiDA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,QAAQ,EAAEC,QAAQ,EAAEC,QAAQ,EAAEC,MAAM,EAAEC,UAAU,EAAEC,QAAQ,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,cAAc;AAG9G,eAAe;EACbC,UAAU,EAAE;IACVR,QAAQ;IACRC,QAAQ;IACRC,QAAQ;IACRC,MAAM;IACNC,UAAU;IACVC,QAAQ;IACRC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIX,GAAG,CAAC,CACjB;MAAEY,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,IAAI;MAAEC,IAAI,EAAE,CAAC;MAAEC,YAAY,EAAE,IAAI;MAAEC,SAAS,EAAE,IAAI;MAAEC,WAAW,EAAE;IAAK,CAAC,EACtF;MAAEL,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,IAAI;MAAEC,IAAI,EAAE,CAAC;MAAEC,YAAY,EAAE,IAAI;MAAEC,SAAS,EAAE,IAAI;MAAEC,WAAW,EAAE;IAAK,CAAC,EACtF;MAAEL,EAAE,EAAE