":["<template>\r\n  <div class=\"container\">\r\n    <div v-for=\"light in lights\" :key=\"light.id\" class=\"light\">\r\n      <div class=\"light-wrapper\">\r\n        <div class=\"light-status\">\r\n          <el-text>\r\n            {{ light.name }}\r\n          </el-text>\r\n          <el-text>\r\n            {{ light.isOn===1 ? '已开启' : '已关闭' }}\r\n          </el-text>\r\n        </div>\r\n        <div class=\"light-switch\">\r\n          <el-switch\r\n              v-model=\"light.isOn\"\r\n              :active-value=\"1\"\r\n              :inactive-value=\"0\"\r\n              active-color=\"#13ce66\"\r\n              inactive-color=\"#ff4949\">\r\n          </el-switch>\r\n        </div>\r\n        <div class=\"light-timer\">\r\n          <el-select\r\n              v-model=\"light.isTimerOnOff\"\r\n              placeholder=\"定时开关\">\r\n              <el-option\r\n                  v-for=\"item in onOff\"\r\n                  :key=\"item\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n              </el-option>\r\n          </el-select>\r\n          <el-select\r\n              v-model=\"light.timerTime\"\r\n              placeholder=\"定时时间\">\r\n              <el-option\r\n                  v-for=\"item in timerChoices\"\r\n                  :key=\"item\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n              </el-option>\r\n          </el-select>\r\n        </div>\r\n        <div class=\"light-timer-choose\">\r\n          <el-button @click=\"cancelTime(light)\">取消</el-button>\r\n          <el-button @click=\"confirmTime(light)\">确认</el-button>\r\n        </div>\r\n        <div class=\"light-timer-countdown\">\r\n          <el-text v-if=\"light.timerStatus===true && light.isTimerOnOff===1\">\r\n            kai\r\n          </el-text>\r\n          <el-text v-else-if=\"light.timerStatus===true && light.isTimerOnOff===0\">\r\n            guan\r\n          </el-text>\r\